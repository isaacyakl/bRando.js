{
	"name": "brandojs",
	"version": "0.1.3",
	"description": "A CSS background changer capable of automatically transitioning in random or sequential order.",
	"main": "dist/bRando.js",
	"scripts": {
		"dev": "conc -k -c \"auto\" -p \"{name}:\" -n \"JEST,WEBPACK\" \"npm:watch-test\" \"npm:dev-webpack\"",
		"dev-webpack": "webpack-dev-server --mode development",
		"watch-test": "jest --watchAll --verbose --coverage",
		"watch-docs": "conc -k -c \"auto\" -p \"{name}:\" -n \"TYPEDOC,LIVE\" \"typedoc --watch\" \"live-server --open=/dist/docs\"",
		"lint": "npx update-browserslist-db@latest",
		"build": "npm run lint && jest --coverage && webpack --mode production && typedoc",
		"publish": ""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/isaacyakl/brandojs.git"
	},
	"keywords": [
		"background changer",
		"background rotator",
		"CSS3",
		"background-image",
		"background-color",
		"javascript",
		"randomizer",
		"random",
		"sequential"
	],
	"author": "Isaac 'yak' L. <i@isaacyakl.com> (https://www.isaacyakl.com/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/isaacyakl/brandojs/issues"
	},
	"homepage": "https://github.com/isaacyakl/brandojs#readme",
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@types/jest": "^29.2.4",
		"babel-jest": "^29.3.1",
		"babel-loader": "^9.1.0",
		"concurrently": "^7.6.0",
		"copy-webpack-plugin": "^11.0.0",
		"html-webpack-plugin": "^5.5.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"live-server": "^1.2.2",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.23",
		"typescript": "^4.9.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.11.1"
	}
}
